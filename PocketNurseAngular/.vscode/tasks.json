{
    // dotnet publish -o ~/Projects/PocketNurseAngularPublish
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/PocketNurseAngular.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/PocketNurseAngular.csproj",
                "-o ${workspaceFolder}/../../PocketNurseAngularPublish"
            ]
        }
    ]
}